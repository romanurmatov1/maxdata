.info-blocks{
    display: flex;
    align-items: center;
    margin-bottom: 30px;
}
.info-blocks i.icon-info-blocks{
    margin-top: 0;
}
.online-forms{
    display: flex;
    flex-wrap: wrap;
}
.of-items{
    display: flex;
    width: 33.3333%;
    padding: 5px;
}
.of-title{
    margin: 0 0 20px;
    text-align: center;
    font-size: 20px;
}
.shadow{
    box-shadow: 0 .5rem 1rem rgba(0,0,0,.15)!important;
}
.of-items .shadow{
    width: 100%;
    padding: 20px 15px;
}
.button-group{
    display: flex;
    flex-direction: column;
}
.button-group a{
    display: block;
    text-align: center;
    text-decoration: none;
    color: #fff;
    background: #0fcdff;
    padding: 15px 0;
    border-radius: 4px;
    font-size: 16px;
    margin-bottom: 10px;
}
.navbar-brand{
    overflow: hidden;
    max-width: 280px;
    padding: 0;
    margin-top: 10px;
    height: 60px;
}
.navbar-brand img{
    max-width: 100%;
}
ul.list{
    padding-left: 30px;
}
ul.list,
ul.list li{
    list-style-type: disc;
}
.navbar-brand{
    margin-top: 0;
    overflow: hidden;
    height: 100%;
}

